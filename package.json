{
  "name": "tennis_3.0",
  "version": "1.0.0",
  "description": "Tennis court app with ES6",
  "license": "MIT",
  "scripts": {
    "build": "babel-node server.js",
    "client": "webpack-dev-server --open --config webpack.dev.js",
    "coverage": "jest --coverage --verbose --env=jsdom",
    "flow": "flow",
    "heroku-postbuild": "echo Skip build on Heroku",
    "lint:js": "eslint . --ext .js,.jsx",
    "server": "babel-node server.js",
    "start": "babel-node server.js",
    "test": "jest --verbose --env=jsdom",
    "test:watch": "jest --watch --env=jsdom",
    "dev": "concurrently \"npm run server\" \"npm run client\""
  },
  "babel": {
    "env": {
      "test": {
        "plugins": [
          "@babel/plugin-transform-runtime"
        ]
      }
    },
    "plugins": [
      "@babel/plugin-proposal-class-properties",
      "@babel/plugin-proposal-export-default-from"
    ],
    "presets": [
      "@babel/preset-env",
      "@babel/preset-react",
      "@babel/preset-flow"
    ]
  },
  "dependencies": {
    "@babel/cli": "^7.11.6",
    "@babel/core": "^7.11.6",
    "@babel/node": "^7.10.5",
    "@babel/plugin-proposal-class-properties": "^7.10.4",
    "@babel/plugin-proposal-decorators": "^7.10.5",
    "@babel/plugin-proposal-do-expressions": "^7.10.4",
    "@babel/plugin-proposal-export-default-from": "^7.10.4",
    "@babel/plugin-proposal-export-namespace-from": "^7.10.4",
    "@babel/plugin-proposal-function-bind": "^7.11.5",
    "@babel/plugin-proposal-function-sent": "^7.10.4",
    "@babel/plugin-proposal-json-strings": "^7.10.4",
    "@babel/plugin-proposal-logical-assignment-operators": "^7.11.0",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.10.4",
    "@babel/plugin-proposal-numeric-separator": "^7.10.4",
    "@babel/plugin-proposal-optional-chaining": "^7.11.0",
    "@babel/plugin-proposal-pipeline-operator": "^7.10.5",
    "@babel/plugin-proposal-throw-expressions": "^7.10.4",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-syntax-import-meta": "^7.10.4",
    "@babel/plugin-transform-flow-strip-types": "^7.10.4",
    "@babel/plugin-transform-runtime": "^7.11.5",
    "@babel/preset-env": "^7.11.5",
    "@babel/preset-flow": "^7.10.4",
    "@babel/preset-react": "^7.10.4",
    "@material-ui/core": "^4.11.0",
    "acorn": "^6.4.1",
    "axios": "^0.20.0",
    "babel-eslint": "^9.0.0",
    "babel-loader": "^8.1.0",
    "babel-preset-flow": "^6.23.0",
    "bcrypt": "^5.0.0",
    "body-parser": "^1.19.0",
    "connect-mongo": "^3.2.0",
    "css-loader": "^4.3.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-loader": "4.0.2",
    "eslint-plugin-flowtype": "^3.13.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-react": "^7.20.6",
    "express": "^4.17.1",
    "express-session": "^1.17.1",
    "file-loader": "^3.0.1",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^5.5.0",
    "image-webpack-loader": "^6.0.0",
    "mongoose": "^5.10.5",
    "node-fetch": "^2.6.1",
    "nodemon": "^1.19.4",
    "puppeteer": "^1.20.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-redux": "^6.0.0",
    "react-router": "^4.0.0",
    "react-router-dom": "^4.3.1",
    "react-router-middleware": "^0.1.0",
    "react-router-redux": "^4.0.8",
    "redux": "^3.6.0",
    "redux-thunk": "^2.3.0",
    "regenerator-runtime": "^0.13.7",
    "sass-loader": "^7.3.1",
    "serve-favicon": "^2.5.0",
    "style-loader": "1.2.1",
    "url-loader": "^1.1.2",
    "webpack": "^4.44.1",
    "webpack-dev-middleware": "^3.7.2",
    "webpack-hot-middleware": "^2.25.0",
    "webpack-merge": "^4.2.2"
  },
  "devDependencies": {
    "babel-jest": "^24.9.0",
    "concurrently": "^5.3.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.4",
    "enzyme-to-json": "^3.5.0",
    "fetch-mock": "^7.7.3",
    "flow-bin": "^0.94.0",
    "jest": "^24.9.0",
    "moxios": "^0.4.0",
    "redux-mock-store": "^1.5.4",
    "redux-test-utils": "^0.3.0",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0"
  },
  "engines": {
    "node": "11.10.1"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/trebek1/tennis_3.0"
  },
  "heroku-run-build-script": true
}
